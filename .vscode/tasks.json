{
  "version": "2.0.0",
  "tasks": [
    {
      "label": "build-dotnetdebug",
      "type": "process",
      "command": "dotnet",
      "args": [
        "build",
        "${workspaceFolder}/src/DotnetDebug/DotnetDebug.csproj",
        "-c",
        "Debug"
      ],
      "problemMatcher": "$msCompile"
    },
    {
      "label": "build-dotnetdebug-tests",
      "type": "process",
      "command": "dotnet",
      "args": [
        "build",
        "${workspaceFolder}/tests/DotnetDebug.Tests/DotnetDebug.Tests.csproj",
        "-c",
        "Debug"
      ],
      "problemMatcher": "$msCompile"
    }
  ]
}
